<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefWidth="350" prefHeight="256" xmlns:fx="http://javafx.com/fxml/1">
	<VBox alignment="CENTER">
		<Label alignment="CENTER" prefWidth="350" prefHeight="40" text="F12B-AGN Dungeon"  textFill="#000" style="-fx-font-size: 29;"/>
		<Button prefWidth="200" prefHeight="40" fx:id="levelSelectBtn" onAction="#handleLevelSelectBtn" text="Select Level" />
		<Button prefWidth="200" prefHeight="40" fx:id="quitBtn" onAction="#handleQuitBtn" text="Quit" />
	</VBox>
</StackPane>
